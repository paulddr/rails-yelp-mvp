<div class= "container pt-5" >
<%= form_for (@restaurant) do |f| %>
<div class="form-group">
  <p><%= f.label :name %>
  <p><%= f.text_field :name %></p>
</div>
<div class="form-group">
  <p><%= f.label :category %></p>
  <p><%= f.select :category, (["chinese","italian","japanese","french","belgian"]) %></p>
</div>
<div class="form-group">
   <p><%= f.label :address %></p>
   <p><%= f.text_field :address %></p>
</div>
<div class="form-group">
  <p><%= f.label :phone_number %></p>
  <p><%= f.number_field :phone_number %></p>
</div>
  <button type="submit" class="btn btn-primary">Submit</button>

<% end %>
<br>
<%= link_to "Go back mamen", restaurants_path, class:"btn btn-primary w-25" %>
</div>


